{
  "name": "forward135",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node index.js",
    "prestart": "webpack --config ./client/webpack.config.js --progress --colors",
    "build": "webpack --config ./client/webpack.config.js --progress --colors",
    "karma": "karma start ./client/karma.config.js",
    "karma:watch": "npm run karma -- --auto-watch --no-single-run",
    "karma:all": "npm run karma -- --browsers=PhantomJS,Chrome,Firefox",
    "packages": "npm list --depth=0",
    "package:purge": "rm -rf node_modules",
    "package:reinstall": "npm run package:purge && npm install",
    "package:updates": "./node_modules/.bin/ncu '/^(?!npm-shrinkwrap|jsdom).*$/'",
    "package:upgrade": "npm run package:updates -- -u"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "karma": "^0.13.22",
    "lodash": "^4.11.2",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0",
    "socket.io": "^1.4.6",
    "socket.io-client": "^1.4.6"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.0.1",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "phantomjs-polyfill": "0.0.2",
    "webpack": "^1.13.0"
  }
}
